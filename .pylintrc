[MASTER]
ignore-paths=^\.git.*$,
             .*/docker/.*$,
             migrations,
             .*/migrations/.*$,
             .*/tests/.*$,
             .*/settings.*$,
             .*/base.py
jobs=1

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,too-few-public-methods,unused-import,unused-argument,wrong-import-order,no-member,redefined-outer-name,duplicate-code

[FORMAT]
max-line-length=100

[REPORTS]
output-format=github
reports=no
score=no
